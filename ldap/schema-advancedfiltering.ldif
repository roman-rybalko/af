replace: olcObjectClasses
# common/smtp/bwlist
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.3.1.1.1 NAME 'afSMTPHostAddressBWListEntry' DESC 'System (ou=system) SMTP service Host Address Black List Entry' SUP top STRUCTURAL MUST ( afSMTPBWListHostAddress ) MAY ( afSMTPBWListIsBlack $ afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.3.1.1.2 NAME 'afSMTPHostNameBWListEntry' DESC 'System (ou=system) SMTP service Host Name Black List Entry' SUP top STRUCTURAL MUST ( afSMTPBWListHostName ) MAY ( afSMTPBWListIsBlack $ afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.3.1.1.3 NAME 'afSMTPCertificateBWListEntry' DESC 'System (ou=system) SMTP service Certificate Black List Entry' SUP top STRUCTURAL MUST ( afSMTPBWListCertificateSubject ) MAY ( afSMTPBWListIsBlack $ afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.3.1.1.4 NAME 'afSMTPSenderMailAddressBWListEntry' DESC 'System (ou=system) SMTP service Sender Mail Address Black List Entry' SUP top STRUCTURAL MUST ( afSMTPBWListSenderMailAddress ) MAY ( afSMTPBWListIsBlack $ afDescription ) )
# system ("S" prefix)
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.1.1.1 NAME 'afSHost' DESC 'System (ou=system) Host' SUP top STRUCTURAL MUST ( afSHostName $ afSHostServiceName $ afSHostRealm ) MAY ( afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.1.1.2 NAME 'afSSMTPDomain' DESC 'System (ou=system) SMTP Domain' SUP top STRUCTURAL MUST ( afSSMTPDomainName $ afSSMTPDomainClientName ) MAY ( afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.1.1.3 NAME 'afSClient' DESC 'System (ou=system) Client' SUP top STRUCTURAL MUST ( afSClientName $ afSClientRealm ) MAY ( afDescription ) )
# system/service
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.1.2 NAME 'afSService' DESC 'System (ou=system) Service' SUP top ABSTRACT MUST ( afSServiceName ) MAY ( afDescription ) )
# system/smtp
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.1.2.1 NAME 'afSSMTPService' DESC 'System (ou=system) SMTP Service' SUP afSService STRUCTURAL MUST ( afSSMTPTrustedPort $ afSSMTPSpamTrapProbabilityNumerator $ afSSMTPSpamTrapProbabilityDenominator
 $ afSSMTPMaxMessageSize $ afSSMTPTrustedMessageSize ) MAY ( afSSMTPDNSBLDomainName ) )
# user ("U" prefix)
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.1.1 NAME 'afUService' DESC 'User (ou=user) Service' SUP top STRUCTURAL MUST ( afUServiceName ) MAY ( afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.1.2 NAME 'afUSRealm' DESC 'User (ou=user) Service Realm' SUP top STRUCTURAL MUST ( afUServiceRealm ) MAY ( afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.1.3 NAME 'afUSRClient' DESC 'User (ou=user) Service Realm Client' SUP top STRUCTURAL MUST ( afUClientName $ afUClientAdminMailAddress $ afUClientTechMailAddress ) MAY ( afDescription ) )
# user/smtp
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.2.1 NAME 'afUSMTPDomain' DESC 'User (ou=user) SMTP service Domain' SUP top STRUCTURAL MUST ( afUSMTPDomainName $ afUSMTPDomainAdminMailAddress $ afUSMTPDomainTechMailAddress ) MAY ( afUSMTPDomainAliasName $ afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.2.2 NAME 'afUSMTPDMailBox' DESC 'User (ou=user) SMTP Domain Mail Box' SUP top STRUCTURAL MUST ( afUSMTPDMBLocalPart $ afUSMTPDMBTimeCreated ) MAY ( afUSMTPDMBIsAbsent $ afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.2.3 NAME 'afUSMTPMailExchanger' DESC 'User (ou=user) SMTP Mail Exchanger' SUP top STRUCTURAL MUST ( afUSMTPMXHostName $ afUSMTPMXTCPPort ) MAY ( afUSMTPMXAuthUser $ afUSMTPMXAuthPassword $ afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.2.4 NAME 'afUSMTPBackupTarget' DESC 'User (ou=user) SMTP Backup Target' SUP top STRUCTURAL MUST ( afUSMTPBackupMailAddress ) MAY ( afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.2.5 NAME 'afUSMTPSpamTarget' DESC 'User (ou=user) SMTP Spam Target' SUP top STRUCTURAL MUST ( afUSMTPSpamMailAddress ) MAY ( afDescription ) )
# user/mdb (Message DataBase)
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.3.1 NAME 'afUMDBMessage' DESC 'User (ou=user) MDB (Message DataBase) Message' SUP top ABSTRACT MUST ( afUMDBMessageId $ afUMDBMessageSenderMailAddress $ afUMDBMessageTimeCreated ) MAY ( afDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.3.1.1 NAME 'afUMDBMessageIncoming' DESC 'User (ou=user) MDB (Message DataBase) Message Incoming' SUP afUMDBMessage STRUCTURAL MAY ( afUMDBMessageSpamDescription ) )
olcObjectClasses: ( 1.3.6.1.4.1.40547.1.2.2.3.1.2 NAME 'afUMDBMessageOutgoing' DESC 'User (ou=user) MDB (Message DataBase) Message Outgoing' SUP afUMDBMessage STRUCTURAL )
