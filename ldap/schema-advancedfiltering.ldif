olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.13 NAME 'afSpamDescription' DESC 'Message verdict info' SUP afDescription )
-
replace: olcObjectClasses
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.1.1 NAME 'afHost' SUP top STRUCTURAL MUST ( afHostName $ afServiceName $ afClientRealm ) MAY ( afDescription ) )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.1.2 NAME 'afDomain' SUP top STRUCTURAL MUST ( afDomainName $ afClientName ) MAY ( afDescription ) )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.1.3 NAME 'afClient' SUP top STRUCTURAL MUST ( afClientName $ afClientRealm ) MAY ( afDescription ) )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.2.1 NAME 'afService' SUP top STRUCTURAL MUST ( afServiceName ) MAY ( afDescription ) )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.2.3 NAME 'afMailBox' SUP top STRUCTURAL MUST ( afRecipientAddress $ afTimeCreated $ afNotExists ) MAY ( afDescription ) )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.2.4 NAME 'afMessage' SUP top STRUCTURAL MUST ( afMessageId $ afSenderAddress $ afTimeCreated ) MAY ( afDescription ) )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.2.4.1 NAME 'afMessageIncoming' SUP afMessage STRUCTURAL MUST ( afIsSpam ) MAY ( afSpamDescription ) )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.2.4.2 NAME 'afMessageOutgoing' SUP afMessage STRUCTURAL )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.2.5 NAME 'afMessageThread' SUP top STRUCTURAL MUST ( afThreadIndex $ afSenderAddress $ afTimeCreated ) MAY ( afDescription ) )
olcObjectClasses:  ( 1.3.6.1.4.1.40547.1.2.2.6 NAME 'afDomainSettings' SUP top STRUCTURAL MUST ( afDomainName ) MAY ( afDescription ) )

