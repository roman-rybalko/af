#                                                .2 (object) .1 (configuration) .x (object id)
#                                                            .2 (services)
replace: olcAttributeTypes
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.1 NAME 'afCaseExactString' DESC 'Case Exact string' EQUALITY caseExactIA5Match SYNTAX 1.3.6.1.4.1.1466.115.121.1.26 )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.2 NAME 'afInteger' DESC 'Integer' EQUALITY integerMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.3 NAME 'afOrderingInteger' DESC 'Integer with ordering' EQUALITY integerMatch ORDERING integerOrderingMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.27 )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.4 NAME 'afBoolean' DESC 'Boolean' EQUALITY booleanMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.7 )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.5 NAME 'afTimeNative' DESC 'Time' EQUALITY generalizedTimeMatch ORDERING generalizedTimeOrderingMatch SYNTAX 1.3.6.1.4.1.1466.115.121.1.24 )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.6 NAME 'afName' DESC 'Name' SUP name )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.7 NAME 'afDescription' DESC 'Description' SUP description )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.8 NAME 'afHost' DESC 'Server name/address' SUP host )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.9 NAME 'afRealm' DESC 'Name' SUP name )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.1.10 NAME 'afMailAddress' DESC 'E-Mail address' SUP mail )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.1 NAME 'afHostName' DESC 'Server name' SUP afHost )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.2 NAME 'afServiceName' DESC 'Service name/id' SUP afName )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.3 NAME 'afClientRealm' DESC 'Realm' SUP afRealm )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.4 NAME 'afTime' DESC 'UNIX time. Seconds from epoch.' SUP afOrderingInteger )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.5 NAME 'afMessageId' DESC 'Message-Id header' SUP afCaseExactString )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.6 NAME 'afThreadIndex' DESC 'Thread-Index header' SUP afCaseExactString )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.7 NAME 'afDomainName' DESC 'Domain name' SUP host )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.8 NAME 'afClientName' DESC 'Domain name' SUP afName )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.9 NAME 'afSenderAddress' DESC 'Sender e-mail' SUP afMailAddress )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.10 NAME 'afTimeCreated' DESC 'Creation time' SUP afTime )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.11 NAME 'afRecipientAddress' DESC 'Recipient e-mail' SUP afMailAddress )
olcAttributeTypes: ( 1.3.6.1.4.1.40547.1.1.2.11 NAME 'afNotExists' DESC 'MailBox does not exist' SUP afBoolean )
