dn: cn=config
changetype: modify
add: olcTLSCACertificateFile
olcTLSCACertificateFile: /usr/local/etc/ssl/ca.crt
-
add: olcTLSCertificateFile
olcTLSCertificateFile: /usr/local/etc/ssl/h01.hosts.advancedfiltering.net.crt
-
add: olcTLSCertificateKeyFile
olcTLSCertificateKeyFile: /usr/local/etc/ssl/h01.hosts.advancedfiltering.net.key
-
add: olcTLSCRLFile
olcTLSCRLFile: /usr/local/etc/ssl/crl.pem
-
add: olcTLSVerifyClient
olcTLSVerifyClient: true
-
add: olcTLSCRLCheck
olcTLSCRLCheck: all
