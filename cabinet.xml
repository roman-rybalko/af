<?xml version="1.0" ?>
<?xml-stylesheet href="default.xsl" type="text/xsl" ?>
<root>
	<menu>
		<item name="stats" link="http://cgi/stats" />
		<item name="settings" link="http://cgi/settings" />
		<item name="trace" link="http://cgi/trace" />
		<item name="trash" link="http://cgi/trash" />
		<item name="cabinet" selected="1" />
		<item name="description" link="description.xml" />
	</menu>
	<form name="login" link="http://cgi">
		<input name="login" type="text"/>
		<input name="password" type="text"/>
	</form>
	<form name="register" link="http://cgi">
		<input name="login" type="text"/>
		<input name="password1" type="text"/>
		<input name="password2" type="text"/>
		<input name="email" type="text"/>
	</form>
	<form name="settings" link="http://cgi">
		<input name="text1" type="text"/>
		<input name="drop1" type="drop">
			<option value="value1"/>
			<option value="value2"/>
			<option value="value3"/>
		</input>
		<input name="radio1" type="radio">
			<option value="value1"/>
			<option value="value2"/>
			<option value="value3"/>
		</input>
		<input name="multi1" type="multi">
			<option value="value1"/>
			<option value="value2"/>
			<option value="value3"/>
		</input>
		<input name="check1" type="check"/>
	</form>
</root>
